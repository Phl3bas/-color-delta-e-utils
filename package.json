{
  "name": "@color-delta-e/utils",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "Utils package for color-delta-e",
  "author": "Sepp Wikramaratna-Clarke <https://github.com/Phl3bas>",
  "license": "MIT",
  "homepage": "https://github.com/Phl3bas/@color-delta-e/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phl3bas/@color-delta-e/utils.git"
  },
  "bugs": "https://github.com/Phl3bas/@color-delta-e/utils/issues",
  "keywords": [
    "color-delta-e",
    "delta-e",
    "deltae",
    "diff",
    "color",
    "colour",
    "color-diff",
    "color-diffing"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "color-delta-e": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "color-delta-e": "^1.0.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.11.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.1",
    "vitest": "^0.22.1",
    "yewtils": "^0.0.2"
  }
}
